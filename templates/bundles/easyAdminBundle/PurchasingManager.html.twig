    <h3 class="page-title text-truncate text-dark font-weight-medium mb-1">Hello {{ app.user.firstName }} {{ app.user.lastName }}</h3>
    <a >Welcome to your administration space as purchasing manager </a>
    <div class="row">
        <!-- Column -->
        <div class="col-md-6 col-lg-3 col-xlg-3">
            <div class="card card-hover">
                <div class="p-2 bg-primary text-center">
