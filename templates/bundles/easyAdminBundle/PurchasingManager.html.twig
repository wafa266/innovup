            </div>
        </div>
        <!-- Column -->
        <div class="col-md-6 col-lg-3 col-xlg-3">
            <div class="card card-hover">
                <div class="p-2 bg-cyan text-center">
                    <h3 class="font-light text-white"> {% set somme = 0 %}
                        {% set produit = 0 %}
                        {% for product in products %}
                            {% set produit =(product.price * product.quantity) %}
                            {% set somme = somme+produit %}
                        {% endfor %}
                        {{ somme|format_currency('TND', {fraction_digit: 3}) }} </h3>
