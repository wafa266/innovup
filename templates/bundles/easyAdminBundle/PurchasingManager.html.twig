{# @var ea \EasyCorp\Bundle\EasyAdminBundle\Context\AdminContext #}
{% extends '@EasyAdmin/page/content.html.twig' %}

{% block page_title %}  {% endblock %}
{% block stylesheets %}
    <link href="{{ asset('vendors/bootstrap/dist/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('build/css/main.css') }}" rel="stylesheet">
    <link href="{{ asset('build/css/style-starter.css') }}" rel="stylesheet">
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.css" />
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
{% endblock %}
{% block page_content %}
<style>
    .card-body {
        flex: 1 1 auto;
        padding: 25px;
    }
    * {
        outline: 0;
    }
    *, ::after, ::before {
        box-sizing: border-box;
    }
    feuille de style de l’agent utilisateur
    div {
        display: block;
    }
    .card {
        position: relative;
        display: flex;
        flex-direction: column;
        min-width: 0;
        word-wrap: break-word;
        background-color: #fff;
        background-clip: border-box;
        border: 0 solid #e9ecef;
        border-radius: .25rem;
    }
    a.link, body {
        color: #7c8798;
    }
    body {
        font-family: Rubik,sans-serif;
        font-size: 1rem;
        font-weight: 400;
        line-height: 1.5;
        background-color: #f9fbfd;
    }
    body, caption {
        text-align: left;
    }

    svg.feather.feather-user-plus {
        float: right;
        color: aqua;
    }
    .p-2.bg-cyan.text-center {
        size: a4;
        width: 285px;
        height: 92px;
    }
    h3.font-light.text-white {
        padding-top: 10px;
        font-size: 30px;
    }
    svg.feather.feather-file-plus {
        float: right;
        color: black;
    }
    svg.bi.bi-cash-coin {
        HEIGHT: 26px;
        WIDTH: 35px;
        FLOAT: RIGHT;
        FONT-SIZE: 88px;
        COLOR: cornsilk;
    }


</style>
